function run() {
  let formAction = document.querySelector("div#form-action")
  formAction.innerHTML = ''
  formAction.insertAdjacentHTML('beforebegin', "<%= j render partial: 'job_informations/show', locals: { job_information: @new_job_information } %>")
  formAction.insertAdjacentHTML('afterbegin', "<%= j render partial: 'job_applications/actions' %>")
}

run()