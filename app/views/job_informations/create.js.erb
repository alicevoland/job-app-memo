function run() {
  let formAction = document.querySelector("div#job-information-form")
  formAction.innerHTML = ''
  formAction.insertAdjacentHTML('beforebegin', "<div id='job-information-<%= @new_job_information.id %>'><%= j render partial: 'job_informations/show', locals: { job_information: @new_job_information } %></div>")
  formAction.insertAdjacentHTML('afterbegin', "<%= j render partial: 'job_applications/actions' %>")
}

run()